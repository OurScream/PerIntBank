<%@pagecontentType="text/html" pageEncoding="UTF-8" %>

<html>
  <head>	<title>创建个人网上银行</title>	</head>
  <body>
  		<form action="create" method ="post">
  		<table border="0" width"238" height="252">
  			<tr><td>   证件号  </td><td><input type="text"name="zjid"></td></tr>
  			<tr><td>  客户登录密码  </td><td><input type="text"name="khdlmm"></td></tr>
  			<tr><td>  客户姓名  </td><td><input type="text"name="khxm"></td></tr>
  			<tr><td>  出生日期  </td><td><input type="text"name="csrq"></td></tr>
  			<tr><td>  婚姻状况  </td><td><input type="text"name="hyzk"></td></tr>
  			<tr><td>  所属名族  </td><td><input type="text"name="ssmz"></td></tr>
  			<tr><td>  证件类型  </td><td><input type="text"name="zjlx"></td></tr>
  			<tr><td>  证件有效起始日期  </td><td><input type="text"name="zjqsrq"></td></tr>
  			<tr><td>  证件有效截止日期  </td><td><input type="text"name="zjjzrq"></td></tr>
  			<tr><td>  客户性别  </td><td><input type="text"name="khxb"></td></tr>
  			<tr><td>  客户国籍  </td><td><input type="text"name="khgj"></td></tr>
  			<tr><td>  移动电话  </td><td><input type="text"name="yddh"></td></tr>
  			<tr><td>  固定电话  </td><td><input type="text"name="gddh"></td></tr>
  			<tr><td>  家庭地址  </td><td><input type="text"name="jtdz"></td></tr>
  			<tr><td>  家庭邮编  </td><td><input type="text"name="jtyb"></td></tr>
  			<tr align="center">
  				<td colspan="2">
  					<input type="submit" value="注  册">
  					&nbsp;&nbsp;&nbsp;&nbsp;
  					<input type="reset" value="取  消">
  				</td>
  			</tr>
  		</table>
  		</form>
  	</body>
</html>